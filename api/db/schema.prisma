datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model WorkflowRun {
  id                 Int    @id @default(autoincrement())
  temporalWorkflowId String
}

model WorkflowRunSource {
  id   Int    @id @default(autoincrement())
  slug String
  // link to workflow run
}
