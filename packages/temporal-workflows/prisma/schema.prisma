generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model WorkflowRun {
  id                 Int    @id @default(autoincrement())
  temporalWorkflowId String @unique
  fileInput          String
  resultId           Int?
}

model WorkflowRunResult {
  id                 Int    @id @default(autoincrement())
  temporalWorkflowId String
  result             Json
}

model WorkflowRunSource {
  id   Int    @id @default(autoincrement())
  slug String
}
